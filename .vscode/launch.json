    "version": "0.2.0",
    "configurations": [ {
        "name": "Azure CLI Debug (Integrated Console)",
        "type": "python",
        "request": "launch",
        "pythonPath": "${config:python.pythonPath}",
        "program": "${workspaceRoot}/../azure-cli/src/azure-cli/azure/cli/__main__.py",
        "cwd": "${workspaceRoot}",
        "args": [
            // "cupertino", "webapp", "bind","-n", "test", "-app", "bwsongapp", "-sql", "bwsongsql", "-db", "bwsongdb", "-g", "bwsonggroup", "-auth", "SECRET", "-user", "bwsong", "-pwd", ""
            "cupertino", "webapp", "bind","-n", "test", "-app", "bwsongapp", "-sql", "bwsongsql", "-db", "bwsongdb", "-g", "bwsonggroup", "-auth", "MSI"
            // "cupertino", "springcloud", "bind", "-g", "shenwe", "-n", "test", "-spc", "springcloud", "-app", "apps", "-mysql", "mysqlserver", "-db", "databases", "-user", "username", "-pwd", "password"
            // "cupertino", "function", "bind", "-g", "shenwe", "-n", "test", "-app", "website", "-func", "func1", "-binding", "in", "-signalr", "sigr"
        ],
        "console": "integratedTerminal",
        "debugOptions": [
            "WaitOnAbnormalExit",
            "WaitOnNormalExit",
            "RedirectOutput"
        ],
        "justMyCode": false
    }]
}